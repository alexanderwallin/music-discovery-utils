{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "import/extensions": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "react/react-in-jsx-scope": 0,
  }
}
